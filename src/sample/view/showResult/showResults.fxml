<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="gridPane" alignment="center" hgap="10" prefHeight="300.0" prefWidth="248.0" stylesheets="/sample/view/showResult/showResults.css" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.showResults.ShowResultsController">
   <columnConstraints>
      <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints valignment="TOP" vgrow="ALWAYS" />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#saveAction" text="save" textAlignment="JUSTIFY" GridPane.rowIndex="2" />

      <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="نتایج" textAlignment="JUSTIFY" textOverrun="CLIP" />
      <TabPane>
         <Tab text="نتایج محاسبات بر اساس دمای خروجی وارد شده">
            <TableView fx:id="tableID" editable="false" prefWidth="248.0" GridPane.rowIndex="1">
               <columns>

                  <TableColumn fx:id="value" prefWidth="75.0" text="مقدار" />
                   <TableColumn fx:id="name" prefWidth="75.0" text="نام" />
               </columns>
            </TableView>
         </Tab>

         <Tab text=" نتایج محاسبات بر اساس دمای هیدرات">
            <TableView fx:id="hydrateTable" editable="false" prefWidth="248.0" GridPane.rowIndex="1">
               <columns>

                  <TableColumn fx:id="hydrateValue" prefWidth="75.0" text="مقدار" />
                  <TableColumn fx:id="hydrateName" prefWidth="75.0" text="نام" />
               </columns>
            </TableView>
         </Tab>

      </TabPane>
   </children>
</GridPane>
