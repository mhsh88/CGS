            <Button fx:id="pipe2" layoutX="436.50" layoutY="245" mnemonicParsing="false" styleClass="pipe" text="pipe 2" />
            <Button fx:id="pipe3" layoutX="436.50" layoutY="340" mnemonicParsing="false" styleClass="pipe" text="pipe 3" />

            <Button layoutX="526.5" layoutY="140.5" mnemonicParsing="false" styleClass="regulator" text="regulator 1" />
            <Button layoutX="526.5" layoutY="235.5" mnemonicParsing="false" styleClass="regulator" text="regulator 2" />
            <Button layoutX="526.5" layoutY="330.5" mnemonicParsing="false" styleClass="regulator" text="regulator 3" />


            <Button layoutX="576.5" layoutY="150" mnemonicParsing="false" styleClass="knee" text="Knee up right" />
